# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#<src>                                      <mnt_point>     <type>      <mnt_flags and options>                             <fs_mgr_flags>
/dev/block/bootdevice/by-name/userdata      /data           f2fs        nosuid,nodev,noatime,discard,data_flush             wait,check,quota,formattable,fileencryption=ice,reservedsize=128M
/dev/block/bootdevice/by-name/userdata      /data           ext4        nosuid,nodev,noatime,discard,noauto_da_alloc        wait,check,quota,formattable,fileencryption=ice,reservedsize=128M
